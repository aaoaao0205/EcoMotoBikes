<h1>ショッピングカート</h1>

<% if @cart.items.any? %>
  <table>
    <thead>
      <tr>
        <th>商品名</th>
        <th>数量</th>
        <th>単価</th>
        <th>合計</th>
      </tr>
    </thead>
    <tbody>
<% @cart_items.each do |cart_item| %>
  <tr>
    <td><%= cart_item.item_name %></td>
    <td><%= cart_item.price %></td>

        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>

